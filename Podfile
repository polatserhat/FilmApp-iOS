platform :ios, '12.0'

target 'FilmApp-iOS' do
  use_frameworks!
  
  # Firebase Kütüphaneleri
  pod 'Firebase/Core'  # Temel Firebase işlevselliği
  pod 'Firebase/Auth'   # Firebase Authentication için
  pod 'Firebase/Firestore'  # Firestore için
  pod 'Firebase/Analytics'  # Firebase Analytics için
end
